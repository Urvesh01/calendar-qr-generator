  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Urvesh01/calendar-qr-generator.git"
  },
  "homepage": "https://github.com/Urvesh01/calendar-qr-generator#readme",
  "bugs": {
